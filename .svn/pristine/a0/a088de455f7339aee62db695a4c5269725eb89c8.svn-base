.box-shadow-2{  background-color:#fff;
  -webkit-box-shadow:0 0 15px #c3c3c4;  
  -moz-box-shadow:0 0 15px #c3c3c4;  
  box-shadow:0 0 15px #c3c3c4;  
  margin:18px 18px 0 18px;
  float:left;
  min-width:95%;

}
.box-shadow-2 .title {
    height:40px;line-height:40px; color:#0889ed;font-size:18px; font-weight:bold;width:98%;margin:auto;
    }
 .box-shadow-2 .content {
    height:auto; color:#0889ed;font-size:14px; width:98%;margin:auto;display: block;
    }
    .box-shadow-2 div[data-wheel="wheel"] {
    overflow-y:scroll;overflow:hidden;height:50px;
    }
  .box-shadow-2 .content ul> li{
    height:60px; width:136px;float:left;background-color:#f2f2f2;border-radius: 4px;margin:7px 6px 7px 6px; text-align:center;
    }
 .box-shadow-2 .content ul > li span:first-child{
    height:36px; width:100%;float:left;text-align:center; line-height:36px;font-size:20px;font-weight:bold;
    }
 .box-shadow-2 .content ul > li span:last-child{
     width:100%;float:left;color:#999999;text-align:center; 
    }
div[data-wheel="wheel"]{
  overflow-y: scroll;
}
div[data-wheel="wheel"]::-webkit-scrollbar{
    width: 6px;
    background-color: rgba(0,0,0,0.4);
}
div[data-wheel="wheel"]::-webkit-scrollbar-thumb{
      border-radius: 6px;
    background-color: rgba(255,255,255,0.3);
}
div[data-wheel="wheel"]::-webkit-scrollbar-track{
    border-radius: 6px;
    background-color: rgba(0,0,0,0.4);
}
 div[data-wheel="wheel"]::-webkit-scrollbar{
    width: 6px;
    background-color: rgba(255,255,255,0.1);
}
 div[data-wheel="wheel"]::-webkit-scrollbar-thumb{
      border-radius: 6px;
    background-color: #00ccff;
}
