html, body 
{
    width:100%;
    height:100%;
    margin:0;
    padding:0;
}

#ToolBar
{
    position:absolute;
    top:50px;
    right:20px;
    height:34px;
    width:220px;
    z-index:999;
    background-color:White;  
    border-radius:5px;  
}
#ToolBar ul
{
    list-style:none;
    margin-top:0px;
    margin-left:5px;
    
}
#ToolBar li
{
    display:inline;
    line-height:32px;
}
ul li img:hover
{
    background-color:#d2e7f9;
    cursor:pointer;
}
ul li img:focus
{
    background-color:#c3e0f9;
}
#MapDiv
{
    position:relative;
    height:100%;
    width:100%;

}
.esriPopup .titleButton.maximize
{
    display:none;
}
.esriPopup .titlePane
{
    background-color:#0b83f3 !important;
}

#LayerControl
{
    z-index:99;
    float: left; 
    margin-top: 10px; 
    background-color: white;
    position: absolute;
    left:20px;
    bottom:20px;   
    border-radius:5px 5px 0 0;
    display:none;
}

#LayerIcon
{
    z-index:99;
    position: absolute;
    left:20px;
    bottom:20px;  
    color:#fff; 
    background-color:#1090f3;
    height:36px;
    width:36px;
    border-radius:5px;
    cursor:pointer;
}

    #LayerIcon span
    {
        margin:4px;
        font-size:26px;
    }

#LegendIcon
{
    z-index:99;
    position: absolute;
    right:20px;
    bottom:20px;  
    color:#fff;  
    background-color:#1090f3;
    height:36px;
    width:36px;
    border-radius:5px;   
    cursor:pointer; 
}
    #LegendIcon span
    {
        margin:4px;
        font-size:26px;
    }

#Legend
{
    z-index:99;
    float: right; 
    margin-top: 10px; 
    background-color: white;
    position: absolute;
    right:20px;
    bottom:20px;   
    border-radius:5px 5px 0 0;
    display:none;
    color:#1090f3;
}

#legendtop
{
    background: -webkit-linear-gradient(center top, #ffffff, #edebeb);
    background: -moz-linear-gradient(center top, #ffffff, #edebeb);
    background: -o-linear-gradient(center top, #ffffff, #edebeb);
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#edebeb));
    background: linear-gradient(center top, #ffffff, #edebeb);
}

    #legendtop table
    {
        margin-left:10px;
    }
    #legendtop td
    {
        padding:2px;
    }

#layerTree
{
    width: 200px; 
    height: 250px;
    overflow:auto;
}
#layerTree::-webkit-scrollbar{
    width: 6px;
    background-color: #e8e9ed;
}
#layerTree::-webkit-scrollbar-thumb{
    border-radius: 6px;
    background-color: #a2cff8;
}
#layerTree::-webkit-scrollbar-track{
    border-radius: 6px;
    background-color: #e8e9ed;
}

#legenddiv
{
    margin-left:12px;
    margin-top:5px;
    width: 188px; 
    height: 160px;
    overflow:auto;
}
#legenddiv::-webkit-scrollbar{
    width: 6px;
    background-color: #e8e9ed;
}
#legenddiv::-webkit-scrollbar-thumb{
    border-radius: 6px;
    background-color: #a2cff8;
}
#legenddiv::-webkit-scrollbar-track{
    border-radius: 6px;
    background-color: #e8e9ed;
}
.titlediv
{
    background: -webkit-linear-gradient(center top, #22a2ec, #0580c7);
    background: -moz-linear-gradient(center top, #22a2ec, #0580c7);
    background: -o-linear-gradient(center top, #22a2ec, #0580c7);
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#22a2ec), to(#0580c7));
    background: linear-gradient(center top, #22a2ec, #0580c7);
    color:white;
    height:30px;
    width: 200px; 
    line-height:30px;
    text-align:center;
    vertical-align:middle;
    border-radius:5px 5px 0 0;
    cursor:pointer;
}
.photoContainer
{
    position:absolute;
    top:5px;
    right:0px;
}
.photobutton
{
    position:absolute;
    right:130px;

}
.dataTable
{
    position:absolute;
    top:0px;
    left:0px;
}
.dataTable td
{
	padding: 2px;    
}
.head
{
    font-weight:bold;
}