html, body 
{
    width:100%;
    height:100%;
    margin:0;
    padding:0;
}

#ToolBar
{
    position:absolute;
    top:60px;
    right:20px;
    height:34px;
    width:220px;
    z-index:999;
    background-color:White;  
    border-radius:5px;  
}
#ToolBar ul
{
    list-style:none;
    margin-top:0px;
    margin-left:5px;
    
}
#ToolBar li
{
    display:inline;
    line-height:32px;
}
ul li img:hover
{
    background-color:#d2e7f9;
    cursor:pointer;
}
ul li img:focus
{
    background-color:#c3e0f9;
}
#MapDiv
{
    position:relative;
    height:100%;
    width:100%;

}
/*#MapDiv_zoom_slider
{
    position:absolute;
    top:20px;
    left:20px;
}*/
.esriPopup .titleButton.maximize
{
    display:none;
}
.esriPopup .titlePane
{
    background-color:#0b83f3 !important;
}
#searchBox
{
    height:38px;margin-left:20px;
}

#searchBoxItems
{
    width:1250px;
    height:32px;
    margin:0;
    padding:0;
}
.mCS-dark-thin.mCSB_scrollTools .mCSB_dragger .mCSB_dragger_bar{ background-color: #0b83f3 !important;}
.mCS-dark-thin.mCSB_scrollTools .mCSB_dragger:hover .mCSB_dragger_bar{ background-color: #0b83f3 !important;}
.mCSB_scrollTools .mCSB_draggerContainer
{
    margin-top:3px;
}
#LayerControl
{
    z-index:99;
    float: left; 
    margin-top: 10px; 
    background-color: white;
    position: absolute;
    left:20px;
    bottom:20px;   
    border-radius:5px 5px 0 0;
    display:none;
}

#LayerIcon
{
    z-index:99;
    position: absolute;
    left:20px;
    bottom:20px;  
    color:#fff; 
    background-color:#0b83f3;
    height:36px;
    width:36px;
    border-radius:5px;
    cursor:pointer;
}

    #LayerIcon span
    {
        margin:4px;
        font-size:26px;
    }

#LegendIcon
{
    z-index:99;
    position: absolute;
    right:20px;
    bottom:20px;  
    color:#fff;  
    background-color:#0b83f3;
    height:36px;
    width:36px;
    border-radius:5px;   
    cursor:pointer; 
}
    #LegendIcon span
    {
        margin:4px;
        font-size:26px;
    }

#Legend
{
    z-index:99;
    float: right; 
    margin-top: 10px; 
    background-color: white;
    position: absolute;
    right:20px;
    bottom:20px;   
    border-radius:5px 5px 0 0;
    color:#1090f3;
}

#legendtop
{
    background: -webkit-linear-gradient(center top, #ffffff, #edebeb);
    background: -moz-linear-gradient(center top, #ffffff, #edebeb);
    background: -o-linear-gradient(center top, #ffffff, #edebeb);
    background: -webkit-gradient(linear, 0 0, 0 100%, from(#ffffff), to(#edebeb));
    background: linear-gradient(center top, #ffffff, #edebeb);
}

    #legendtop table
    {
        margin-left:10px;
    }
    #legendtop td
    {
        padding:2px;
    }

#layerTree
{
    margin-left:10px;
    margin-top:10px;
    width: 150px; 
    max-height: 170px;
    overflow:auto;
}
#layerTree span.indent
{
    margin:0;
}
#layerTree .node-layerTree
{
    padding:0;
}
#layerTree::-webkit-scrollbar{
    width: 6px;
    background-color: #e8e9ed;
}
#layerTree::-webkit-scrollbar-thumb{
    border-radius: 6px;
    background-color: #a2cff8;
}
#layerTree::-webkit-scrollbar-track{
    border-radius: 6px;
    background-color: #e8e9ed;
}

#legenddiv
{
    margin-left:12px;
    margin-top:5px;
    width: 148px; 
    max-height: 100px;
    overflow:auto;
}
#legenddiv_msg
{
    display:none;
}
#legenddiv::-webkit-scrollbar{
    width: 6px;
    background-color: #e8e9ed;
}
#legenddiv::-webkit-scrollbar-thumb{
    border-radius: 6px;
    background-color: #a2cff8;
}
#legenddiv::-webkit-scrollbar-track{
    border-radius: 6px;
    background-color: #e8e9ed;
}
.titlediv
{
    background-color:#0b83f3;
    color:white;
    height:30px;
    width: 160px; 
    line-height:30px;
    text-align:center;
    vertical-align:middle;
    border-radius:5px 5px 0 0;
    cursor:pointer;
}
.photoContainer
{
    position:absolute;
    top:5px;
    right:0px;
}
.photobutton
{
    position:absolute;
    left:0px;
    width:300px;
}
.photoleft
{
    float:left;
    font-size:20px;
    color:#0b83f3;
}
.photoright
{
    float:right;
    font-size:20px;
    color:#0b83f3;
}
.dataTable
{
    position:absolute;
    top:0px;
    left:0px;
}
.dataTable td
{
	padding: 2px;    
}
.head
{
    font-weight:bold;
}

.button_btn {
    width: 60px;
    height: 24px;
    border: 1px solid #078ddb;
    border-radius: 3px;
    line-height: 5px;
    color: #fff;
    background-color:#0b83f3;
    box-shadow: 0px 0px 1px #fff inset;
    box-shadow: 0px 0px 1px #fff inset;
    -webkit-box-shadow: 0px 0px 1px #fff inset;
    -moz-box-shadow: 0px 0px 1px #fff inset;
    -o-box-shadow: 0px 0px 1px #fff inset;
    text-shadow: 1px 1px 0px #0977b7;
    cursor: pointer;
}

    .button_btn:hover {
        box-shadow: 0px 0px 3px #fff inset;
        color: #fff;
    }