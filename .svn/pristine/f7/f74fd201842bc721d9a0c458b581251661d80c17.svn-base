    <script>          
        $(".menuDiv").load("/Manager/Menu");        
        $(function () {
            if ($(window).width() < 1170) {
                $(".small_menu").css({
                    "display": "none"
                });
            } else if ($(window).width() < 1350) {
                
                $(".li1").css({
                    "display": "none"
                });
            }
            GetTime();
            setInterval('GetTime()', 1000); //指定1秒钟局部刷新一次
        });
        GetPartTree(1);
       // setTimeout('setLDT()', 1);
        // setTimeout('setBXT()', 1);
        var mapdata = [];
        mapdata['id'] = '999999';
        mapdata['href'] = 'map';
        mapdata['text'] = "地图";
        setTimeout('clickTreeMenu(mapdata)', 1);
       </script>
</head>
<body>
    @{Html.RenderAction("Menu");}
    <div class="container">
